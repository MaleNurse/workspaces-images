#!/bin/bash

export NVM_DIR="/home/kasm-user/.nvm"
export NODE_VERSION="20.9.0"
. ${NVM_DIR}/nvm.sh
export PATH="${NVM_DIR}/v${NODE_VERSION}/bin:${PATH}"

if [ "$1" ]; then
  /usr/share/code/bin/code $* > /dev/null 2>&1
else
  /usr/share/code/bin/code --no-sandbox --new-window > /dev/null 2>&1
fi
sleep 4
center-active-window
